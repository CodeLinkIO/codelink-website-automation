<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Peregrine Test" parallel="none">
    <!-- Global parameter	-->
    <parameter name="environmentName" value="STAGING"/>
    <parameter name="browser" value="chrome"/>

    <listeners>
        <listener class-name="reportConfig.ExtentTestListener"/>
    </listeners>

    <test name="Contact Us Flow">
        <classes>
            <class name="testcases.ContactUsTest">
                <methods>
                    <include name="Contact_Us_Flow_With_Embedded_Team" />
                </methods>
            </class>
        </classes>
    </test>

    <test name="Services Pages">
        <classes>
            <class name="testcases.ServicesTest">
                <methods>
                    <include name="Services_Links_And_Pages" />
                </methods>
            </class>
        </classes>
    </test>

</suite>